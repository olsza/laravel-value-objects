{
    "name": "olsza/value-objects",
    "description": "Useful values objects",
    "keywords": [
        "olsza",
        "php",
        "value object",
        "value objects",
        "values object",
        "values objects",
        "laravel"
    ],
    "type": "project",
    "require": {
        "php": "^8.0|^8.1",
        "michael-rubel/laravel-formatters": "^4.0.1"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Olsza",
            "email": "olsza@users.noreply.github.com",
            "homepage": "https://olsza.info",
            "role": "Developer"
        },
        {
            "name": "Michael Rubel",
            "email": "contact@observer.name",
            "homepage": "https://github.com/michael-rubel",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Olsza\\ValueObjects\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olsza\\ValueObjects\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "TaxNumber": "Olsza\\ValueObjects\\Facades\\TaxNumber"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
